<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.panshul.scholademy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Scholademy">
        <activity android:name=".Activity.Quiz_result"></activity>
        <activity android:name=".Activity.Quiz" />
        <activity android:name=".Activity.HomeScreen" />
        <activity android:name=".Activity.SignUp" />
        <activity android:name=".Activity.StartQuizActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>